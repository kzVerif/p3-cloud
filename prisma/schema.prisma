generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model dogs {
  id         Int    @id @default(autoincrement())
  dog_name   String @db.VarChar(255)
  owner_name String @db.VarChar(255)
  breed      String @db.VarChar(50)
}
